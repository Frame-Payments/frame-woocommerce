{
    "name": "framepayments/php-sdk",
    "description": "Frame Payments PHP SDK",
    "type": "library",
    "license": "MIT",
    "require": {
      "php": "^8.1 || ^8.2 || ^8.3",
      "guzzlehttp/guzzle": "^7.8 || ^8.0",
      "league/iso3166": "^4.3"
    },
    "require-dev": {
      "phpunit/phpunit": "^10.5",
      "mockery/mockery": "^1.6",
      "phpstan/phpstan": "^1.11",
      "friendsofphp/php-cs-fixer": "^3.64",
      "infection/infection": "^0.29"
    },
    "autoload": {
      "psr-4": {
        "Frame\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Frame\\Tests\\": "tests/"
      }
    },
    "scripts": {
      "test": "phpunit",
      "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
